//Priors from local classifiers
1: Has(U,S) & Predicts(S,A,U,L)-> Is(A,U,L) ^2
1: Has(U,S) & ~Predicts(S,U,A,L)-> ~Is(U,A,L)^2

//Collective Rules for relational signals

1: Joins(U,G) & Joins(V,G) & Is(A,V,L) -> Is(A,U,L) ^2
1: Joins(U,G) & Joins(V,G) & ~Is(A,V,L) -> ~Is(A,U,L) ^2
1: Likes(U,T) & Likes(V,T) & Is(A,V,L) -> Is(A,U,L) ^2
1: Likes(U,T) & Likes(V,T) & ~Is(A,V,L) -> ~Is(A,U,L) ^2

1: Friend(U,V) & Is(A,V,L)-> Is(A,U,L) ^2
1: Friend(U,V) & ~Is(A,V,L)-> ~Is(A,U,L) ^2
1: Friend(V,U) & Is(A,V,L)-> Is(A,U,L) ^2
1: Friend(V,U) & ~Is(A,V,L)-> ~Is(A,U,L) ^2



//Ensure that user has one attribute
1: Is(U,A,+L) = 1

